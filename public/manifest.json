{
  "manifest_version": 3,
  "name": "OneNote",
  "version": "1.0",
  "description": "A Chrome extension for simple note taking.",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "512": "logo.png"
    }
  },
  "icons": [
    {
      "src": "logo.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "host_permissions": []
}
